$(".user-content-content").fadeOut(400, function() {
	$(".user-content-tabs li .active").removeClass('active');
	$(".show-ajax-reviews").addClass('active');
	$(this).empty();
	$(this).append('<%= escape_javascript render file: "reviews/index.html.erb", collection: @reviews %>');
	YouShouldntBeVisibleGuys();
	$(this).fadeIn();
});
